<?xml version="1.0" encoding="utf-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Lincoln Park GC - Style only</name>
    <Snippet/>
    <description>No Placemarks, just Style</description>
    <Style id="hole-style">
      <IconStyle>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/golf.png</href>
        </Icon>
      </IconStyle>
      <BalloonStyle>
        <text>
          <![CDATA[
            Hole $[name]<br/>
            Par $[menspar]<br/>
            $[blueyardage] yards
          ]]>
        </text>
      </BalloonStyle>
    </Style>
  </Document>
</kml>
