      <visibility>0</visibility>
      <description>PhotoOverlay</description>
      <styleUrl>#shared-style</styleUrl>
    </PhotoOverlay>
    <gx:Tour id="po0">
      <name>TR</name>
      <visibility>0</visibility>
      <description>gx:Tour</description>
      <styleUrl>#shared-style</styleUrl>
    </gx:Tour>
  </Document>
</kml>
